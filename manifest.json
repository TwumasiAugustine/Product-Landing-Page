{
	"name": "Klassic Watch",
	"short_name": "KW",
	"description": "A description of your app",
	"start_url": "./index.html",
	"display": "standalone",
	"background_color": "#ffffff",
	"theme_color": "#0073e6",
	"icons": [
		{
			"src": "path-to-your-icon.png",
			"sizes": "192x192",
			"type": "image/png"
		}
	],
	"related_applications": [
		{
			"platform": "web"
		}
	],
	"prefer_related_applications": true,
	"scope": "/",
	"serviceworker": {
		"src": "./service-worker.js",
		"scope": "./"
	},
	"orientation": "portrait",
	"lang": "en-US",
	"dir": "ltr",
	"categories": [
		"e-commerce",
		"productivity"
	],
	"iarc_rating_id": "iarc_rating_id",
	"screenshots": [
		{
			"src": "path-to-your-screenshot.png",
			"sizes": "1280x720",
			"type": "image/png"
		}
	],
	"shortcuts": [
		{
			"name": "Klassic Watch",
			"url": "./index.html",
			"icons": [
				{
					"src": "path-to-your-icon.png",
					"sizes": "192x192",
					"type": "image/png"
				}
			]
		}
	],
	"share_target": {
		"action": "./index.html",
		"method": "GET",
		"enctype": "multipart/form-data",
		"params": {
			"title": "title",
			"text": "text",
			"url": "url"
		}
	},
	"custom": {
		"foo": "bar"
	}
}
