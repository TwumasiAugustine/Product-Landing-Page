{
	"name": "Klassic Watch",
	"short_name": "KW",
	"description": "A description of your app",
	"start_url": "./index.html",
	"display": "standalone",
	"background_color": "#ffffff",
	"theme_color": "#0073e6",
	"icons": [
		{
			"src": "./src/assets/logo.png",
			"sizes": "72x72",
			"type": "image/png",
			"purpose": "maskable"
		},
		{
			"src": "./src/assets/logo.png",
