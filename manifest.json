			"type": "image/png",
			"purpose": "maskable"
		},
		{
			"src": "./src/assets/logo.png",
			"sizes": "144x144",
			"type": "image/png",
			"purpose": "maskable"
		},
		{
			"src": "./src/assets/logo.png",
			"sizes": "192x192",
			"type": "image/png",
			"purpose": "maskable"
		},
		{
			"src": "./src/assets/logo.png",
			"sizes": "512x512",
			"type": "image/png",
			"purpose": "maskable"
		}
	],
	"related_applications": [
		{
			"platform": "web"
		}
	],
	"prefer_related_applications": true,
	"scope": "/",
	"serviceworker": {
		"src": "./service-worker.js",
		"scope": "./"
	},
	"orientation": "portrait",
	"lang": "en-US",
	"dir": "ltr",
	"categories": ["e-commerce", "productivity"],
	"iarc_rating_id": "iarc_rating_id",
	"screenshots": [
		{
			"src": "./src/assets/logo.png",
			"sizes": "1280x720",
			"type": "image/png",
			"purpose": "maskable"
		}
	],
	"shortcuts": [
		{
			"name": "Klassic Watch",
			"url": "./index.html",
			"icons": [
				{
					"src": "./src/assets/logo.png",
					"sizes": "192x192",
					"type": "image/png",
					"purpose": "maskable"
				}
			]
		}
	],
	"permissions": [
		"network",
		"geolocation",
		"notifications",
		"camera",
		"microphone",
		"storage",
		"cookies"
	],
	"share_target": {
		"action": "./index.html",
		"method": "POST",
		"enctype": "application/x-www-form-urlencoded",
		"params": {
			"title": "title",
			"text": "text",
			"url": "url",
			"id": "id"
		}
	},
	"custom": {
		"foo": "bar"
	}
